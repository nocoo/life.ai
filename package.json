{
  "name": "life.ai",
  "private": true,
  "type": "module",
  "scripts": {
    "ut": "bun test --coverage --coverage-reporter=text --coverage-reporter=lcov && bun run scripts/coverage-check.ts",
    "ut:scripts": "bun test scripts/test --coverage --coverage-reporter=text --coverage-reporter=lcov && bun run scripts/coverage-check.ts",
    "lint": "eslint . --ext .ts && bun run --cwd dashboard lint",
    "lint:import": "eslint scripts/import/**/*.ts",
    "lint:verify": "eslint scripts/verify/**/*.ts",
    "prepare": "husky",
    "db:init": "bun run scripts/import/footprint/init.ts",
    "db:load": "bun run scripts/import/footprint/cli.ts load",
    "db:agg": "bun run scripts/import/footprint/cli.ts agg",
    "db:refresh": "bun run scripts/import/footprint/refresh.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/bun": "^1.3.6",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "husky": "^9.1.7",
    "typescript": "^5.7.3"
  }
}
